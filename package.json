{
  "name": "kids-theme",
  "displayName": "KIDS THEME",
  "description": "ðŸŽ¨ A pitch-black theme, designed to follow the color standards of VS Code itself â€” perfect for those who find VS Code themes too light. Also supports .cfg files!",
  "version": "0.3.5",
  "author": {
    "name": "Alan Marques",
    "email": "dev.alanmarques@gmail.com",
    "url": "https://github.com/MarqueesDev"
  },
  "publisher": "KidsTheme",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/MarqueesDev/kids-theme"
  },
  "main": "./out/extension.js",
"activationEvents": [
    "workspaceContains:**/*.js",
    "workspaceContains:**/*.ts",
    "workspaceContains:**/*.json",
    "workspaceContains:**/*.yaml",
    "workspaceContains:**/*.yml",
    "workspaceContains:**/*.xml",
    "workspaceContains:**/*.html",
    "workspaceContains:**/*.css",
    "workspaceContains:**/*.sql",
    "workspaceContains:**/*.cpp",
    "workspaceContains:**/*.py",
    "workspaceContains:**/*.sh",
    "workspaceContains:**/*.bash",
    "workspaceContains:**/*.zsh",
    "workspaceContains:**/*.cfg",
    "workspaceContains:**/*.ini",
    "workspaceContains:**/*.properties",
    "workspaceContains:**/*.env",
    "workspaceContains:**/*.markdown",
    "workspaceContains:**/*.php",
    "workspaceContains:**/*.blade.php"
],
  "icon": "./AlienSpeak.gif",
  "engines": {
    "vscode": "^1.99.0"
  },
  "categories": [
    "Themes"
  ],
  "extensionDependencies": [
    "PKief.material-icon-theme"
  ],
  "contributes": {
    "themes": [
      {
        "label": "KIDS THEME COLORFUL",
        "uiTheme": "vs-dark",
        "path": "./themes/kids-theme-colorful.json"
      },
      {
        "label": "KIDS THEME",
        "uiTheme": "vs-dark",
        "path": "./themes/kids-theme-color-theme.json"
      }
    ]
  },
  "devDependencies": {
    "@types/vscode": "^1.99.1",
    "typescript": "^5.8.3"
  },
  "scripts": {
    "compile": "tsc"
  }
}